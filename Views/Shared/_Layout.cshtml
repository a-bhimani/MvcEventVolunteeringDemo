<body>
<div class="navbar navbar-inverse navbar-fixed-top">
<div class="container">
    <div class="navbar-header">
        @Html.ActionLink("Event Volunteering Portal", "Index", "Volunteers",
                            new { area = "" },
                            new { @class = "navbar-brand bold" })
    </div>
    <div class="navbar-collapse collapse">
        <ul class="nav navbar-nav">
            <li>@Html.ActionLink("Volunteers", "Index", "Volunteers")</li>
            <li>@Html.ActionLink("Events", "Index", "Events")</li>
            <li>@Html.ActionLink("Assignments", "Index", "Volunteerings")</li>
        </ul>
    </div>
</div>
</div>
<div class="container body-content">
@RenderBody()
<hr />
<footer>
    <p>&copy; @DateTime.Now.Year - Event Volunteering Portal</p>
</footer>
</div>
